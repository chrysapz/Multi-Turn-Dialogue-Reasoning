#!/bin/bash

#SBATCH --gres=gpu:0
#SBATCH --job-name=Installnlp
#SBATCH --ntasks=1
#SBATCH --cpus-per-task=3
#SBATCH --time=00:30:00
#SBATCH --mem=32000M
#SBATCH --output=jobs/slurm_output/install_env_%A.out


module purge
module load 2021
